    computeaftervalidation
                    %boolean;      "false"
    defaultfocus    %boolean;      "false"
    showdelimiters  %boolean;      "true"
    allowtabout     %boolean;      "false"
    useappletinbrowser
                    %boolean;      "false"
    runexitingonchange
                    %boolean;      #IMPLIED
    literalize      %boolean;      #IMPLIED
    externaldatasource
                    %boolean;      #IMPLIED
    usenotesstyle   %boolean;      #IMPLIED
